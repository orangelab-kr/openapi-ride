{
  "name": "openapi-ride",
  "description": "Manage kickboard rides on the platform, including start, end and location lookups.",
  "version": "1.8.8",
  "main": "dist/src/index.js",
  "repository": "https://github.com/orangelab-kr/openapi-ride.git",
  "author": "bakjonghun <bakjonghun@hikick.kr>",
  "license": "MIT",
  "scripts": {
    "serve": "yarn serve:dev",
    "serve:dev": "sls offline --stage dev",
    "serve:prod": "sls offline --stage prod",
    "serve:stage": "sls offline --stage stage",
    "invoke:returnedPhotoChecker": "yarn invoke:returnedPhotoChecker:dev",
    "invoke:returnedPhotoChecker:dev": "sls invoke local --stage dev --function returnedPhotoChecker",
    "invoke:returnedPhotoChecker:prod": "sls invoke local --stage prod --function returnedPhotoChecker",
    "invoke:returnedPhotoChecker:stage": "sls invoke local --stage stage --function returnedPhotoChecker",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "deploy": "yarn deploy:stage",
    "deploy:prod": "sls deploy --stage prod",
    "deploy:stage": "sls deploy --stage stage",
    "purge": "yarn purge:stage",
    "purge:prod": "sls remove --stage prod",
    "purge:stage": "sls remove --stage stage"
  },
  "dependencies": {
    "@hikick/openapi-internal-sdk": "^1.6.10",
    "@prisma/client": "4.2.1",
    "@sentry/node": "^7.11.1",
    "@sentry/tracing": "^7.11.1",
    "axios": "^0.27.2",
    "cors": "^2.8.5",
    "dayjs": "^1.11.5",
    "express": "^4.17.2",
    "i18n": "^0.15.0",
    "joi": "^17.5.0",
    "jsonwebtoken": "^8.5.1",
    "nodes2ts": "^3.0.0",
    "serverless-http": "^2.7.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/eslint": "^8.4.6",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/express": "^4.17.13",
    "@types/i18n": "^0.13.3",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.7.13",
    "@types/prettier": "^2.7.0",
    "@types/tsc-watch": "4.2.0",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "copy-webpack-plugin": "^11.0.0",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.5.1",
    "prisma": "4.2.1",
    "serverless": "^3.22.0",
    "serverless-doppler": "^1.0.2",
    "serverless-offline": "^9.2.6",
    "serverless-webpack": "^5.9.0",
    "serverless-webpack-prisma": "^1.0.8",
    "ts-loader": "^9.2.6",
    "tsc-watch": "5.0.3",
    "typescript": "^4.8.2",
    "webpack": "^5.74.0",
    "webpack-node-externals": "^3.0.0"
  }
}
